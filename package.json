{
  "name": "expect-to",
  "version": "0.9.0",
  "description": "Assertion library based on pure functions",
  "keywords": [
    "test",
    "expect",
    "assert",
    "assertion",
    "assertions",
    "tdd",
    "bdd"
  ],
  "main": "dist/index.js",
  "dependencies": {
    "assertion-error": "^1.0.1",
    "custom-util-format": "^1.0.0",
    "expect-to-core": "^0.7.0",
    "is-promise": "^2.0.0",
    "loupe": "0.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "phantomjs-prebuilt": "^2.1.3",
    "pre-commit": "^1.1.2",
    "sinon": "^1.17.3",
    "standard": "^6.0.8",
    "webpack": "^1.12.14"
  },
  "scripts": {
    "test": "npm run test-node && npm run test-web",
    "test-node": "mocha --compilers js:babel-core/register",
    "test-web": "karma start karma.conf.js",
    "lint": "standard src/index.js test.js",
    "build": "rm -rf dist; babel src --out-dir dist",
    "prepublish": "npm run build"
  },
  "pre-commit": [
    "lint",
    "test-node"
  ],
  "author": "Kim Joar Bekkelund <kjbekkelund@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/kjbekkelund/expect-to.git"
  },
  "license": "MIT"
}
