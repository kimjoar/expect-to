{
  "name": "expect-to",
  "version": "0.2.0",
  "description": "Assertion library based on pure functions",
  "keywords": [
    "test",
    "expect",
    "assert",
    "assertion",
    "assertions",
    "tdd",
    "bdd"
  ],
  "main": "dist/index.js",
  "dependencies": {
    "is-promise": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.0.15",
    "babel-core": "^6.0.14",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.14",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.1",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.2.5",
    "phantomjs": "^1.9.18",
    "pre-commit": "^1.1.2",
    "standard": "^5.3.1",
    "webpack": "^1.9.10"
  },
  "scripts": {
    "test": "npm run test-node && npm run test-web",
    "test-node": "mocha --compilers js:babel-core/register; exit 0",
    "test-web": "karma start karma.conf.js",
    "lint": "standard src/index.js test.js",
    "build": "rm -rf dist; babel src --out-dir dist",
    "prepublish": "npm run build"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "author": "Kim Joar Bekkelund <kjbekkelund@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/kjbekkelund/expect-to.git"
  },
  "license": "MIT"
}
